Options -Indexes 
ErrorDocument 404 /404.php


RewriteCond %{HTTP_HOST} ^dveri-provance.ru$
RewriteRule ^robots.txt$ /robots-main.txt [L]
RewriteCond %{HTTP_HOST} ^old.dveri-provance.ru$
RewriteRule ^robots.txt$ /robots-subdomain.txt [L]


<IfModule mod_php5.c>
	php_flag allow_call_time_pass_reference 1
	php_flag session.use_trans_sid off

	#php_value display_errors 1

	#php_value mbstring.internal_encoding UTF-8
</IfModule>


	php_value max_input_vars 11000
	php_value pcre.jit 0
	php_value pcre.recursion_limit 100000

<IfModule mod_rewrite.c>

	Options +FollowSymLinks
	RewriteEngine On


	RewriteCond %{HTTPS} off
	RewriteCond %{HTTP:X-Forwarded-Proto} !https
	RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

	RewriteCond %{REQUEST_URI} ^/katalog/dveri-po-ostekleniyu/so-steklom-lakobel/$
	RewriteRule ^.*$ https://dveri-provance.ru/articles/dveri-so-steklom-lakobel/ [R=301,L]

	RewriteCond %{REQUEST_URI} ^/katalog/dveri-po-materialam/dveri-iz-massiva/$
	RewriteRule ^.*$ https://dveri-provance.ru/articles/dveri_iz_lvl_brusa_osobennosti_i_preimushtestva/ [R=301,L]

	RewriteCond %{REQUEST_URI} ^katalog/belye-dveri/$
	RewriteRule ^.*$ https://dveri-provance.ru/shop/belye-dveri/ [R=301,L]

	#добавлено 10.08 удаление цепочки редиректов
	RewriteCond %{REQUEST_URI} ^/contacts$ [NC]
	RewriteRule ^(.*)$ https://dveri-provance.ru/contacts/ [L,R=301]

	RewriteCond %{REQUEST_URI} ^/oplata$ [NC]
	RewriteRule ^(.*)$ https://dveri-provance.ru/services/delivery/ [R=200,L]

	RewriteRule ^uslugi$ https://dveri-provance.ru/services/? [L,R=301]

	RewriteRule ^ustanovka$ https://dveri-provance.ru/services/ustanovka-mezhkomnatnyh-dverey/? [L,R=301]

	RewriteRule ^about_us$ https://dveri-provance.ru/about/? [L,R=301]

	#добавлено 02.08.2023
	RewriteCond %{HTTP_HOST} ^www\.dveri-provance\.ru [NC]
	RewriteRule ^(.*)$ https://dveri-provance.ru/$1 [L,R=301]

	#добавлено 20.08.2023	
	####RewriteRule ^shop/(.*)$ /mezhkomnatnye-dveri/$1 [R=301,L]
	

###

	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-l
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !/bitrix/urlrewrite.php$
	RewriteRule ^(.*)$ /bitrix/urlrewrite.php [L]
	RewriteRule .* - [E=REMOTE_USER:%{HTTP:Authorization}]




</IfModule>


<IfModule mod_expires.c>
	ExpiresActive on
	ExpiresByType image/jpeg "access plus 3 day"
	ExpiresByType image/gif "access plus 3 day"
</IfModule>

